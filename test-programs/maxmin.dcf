class Program {
	int A[100];

	int read_int() {
		int value, sign;
		value = 0;
		sign = 1;
		for i = 0 , 100 {
			int curr;
			curr = callout("getchar");
			if (curr == 45 && i == 0) {
				sign = -1;
				continue;
			}
			if (curr < 48) {
				break;
			}
			value = 10 * value + (curr - 48);
		}
		return sign * value;
	}

	void write_int(int n) {
		callout("printf", "%d", n);
	}

	void ignore() {
		callout("getchar");
	}

	int get_min(int n) {

	}

	void main() {
		int n, s, min, max;

		n = read_int();
		for i = 0 , n {
			A[i] = read_int();
		}
		min = A[0];
		max = A[0];
		for i = 0 , n {
			if (A[i] < min) {
				min = A[i];
			}
			if (A[i] > max) {
				max = A[i];
			}
		}
		write_int(min);
		write_int(max);
	}
}