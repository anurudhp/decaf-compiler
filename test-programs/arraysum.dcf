class Program {
	int A[100];

	int read_int() {
		int value, sign;
		value = 0;
		sign = 1;
		for i = 0 to 100 {
			int curr;
			curr = callout("getchar");
			if (curr == 45 && i == 0) {
				sign = -1;
				continue;
			}
			if (curr < 48) {
				break;
			}
			value = 10 * value + (curr - 48);
		}
		return sign * value;
	}

	void write_int(int n) {
		callout("printf", "%d", n);
	}

	void ignore() {
		callout("getchar");
	}

	int get_sum(int n) {
		int sum;
		sum = 0;
		for i = 0 to n {
			sum += A[i];
		}
		return sum;
	}

	void main() {
		int n, s;
		n = read_int();
		callout (scanf, "%d", n);
		for i = 0 to n {
			A[i] = read_int();
		}
		s = get_sum(n);
		write_int(s);
	}
}