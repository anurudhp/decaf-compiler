class Program {
	int A[100];

	int get_sum(int n) {
		int sum;
		sum = 0;
		for i = 0 , n {
			sum += A[i];
		}
		return sum;
	}

	void main() {
		int n, s;
		n = callout("read_int");
		for i = 0 , n {
			A[i] = callout("read_int");
		}
		s = get_sum(n);
		callout("write_int", s);
		callout("write_char", '\n');
	}
}