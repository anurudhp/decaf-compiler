class Program {
	int A[100];

	void write(int x) {
		callout("write_int", x);
		callout("write_char", 10);
	}

	int bubble_sort(int n) {
		int temp;
		
		write(n);
		for i = 0 , n {
			write(i);
			for j = 0 , n - i - 1 {
				write(j);
				if (A[j] > A[j + 1]) {
					temp = A[j];
					A[j] = A[j + 1];
					A[j + 1] = temp;
				}
			}
		}
	}

	void main() {
		int n, s;
		n = callout("read_int");
		for i = 0 , n {
			A[i] = callout("read_int");
		}
		bubble_sort(n);
		for i = 0 , n {
			callout("write_int", A[i]);
			callout("write_char", 32); // SPACE
		}
		callout("write_char", 10); // LF
	}
}