class Program {
	int A[100];

	int read_int() {
		int value, sign;
		value = 0;
		sign = 1;
		for i = 0 , 100 {
			int curr;
			curr = callout("getchar");
			if (curr == 45 && i == 0) {
				sign = -1;
				continue;
			}
			if (curr < 48) {
				break;
			}
			value = 10 * value + (curr - 48);
		}
		return sign * value;
	}

	void write_int(int n) {
		callout("printf", "%d", n);
	}

	void ignore() {
		callout("getchar");
	}

	void main() {
		int n, max1, max2, temp;
		n = read_int();
		for i = 0 , n {
			A[i] = read_int();
		}
		// assume n >= 2
		max1 = A[0];
		max2 = A[1];
		if (max1 < max2) {
			temp = max1;
			max1 = max2;
			max2 = temp;
		}
		for i = 2 , n {
			if (A[i] > max2) {
				max2 = A[i];
			}
			if (max1 < max2) {
				temp = max1;
				max1 = max2;
				max2 = temp;
			}
		}
		write_int(max2);
	}
}